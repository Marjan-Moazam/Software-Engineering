{
    "ConnectionStrings": {
        "DefaultConnection": "Server=localhost;Database=ETLHubspotDb;Trusted_Connection=True;TrustServerCertificate=True;"
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console",
            "Serilog.Sinks.MSSqlServer",
            "Serilog.Sinks.File"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "System": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "restrictedToMinimumLevel": "Information",
                    "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
                }
            },
            {
                "Name": "MSSqlServer",
                "Args": {
                    "connectionString": "Server=.;Database=ETLHubspotDb;Trusted_Connection=True;TrustServerCertificate=True;",
                    "tableName": "Logs",
                    "schemaName": "Hubspot",
                    "autoCreateSqlTable": true,
                    "restrictedToMinimumLevel": "Information",
                    "columnOptionsSection": {
                        "additionalColumns": [
                            {
                                "ColumnName": "SourceContext",
                                "DataType": "nvarchar",
                                "DataLength": 256,
                                "AllowNull": true
                            },
                            {
                                "ColumnName": "RequestPath",
                                "DataType": "nvarchar",
                                "DataLength": 1000,
                                "AllowNull": true
                            },
                            {
                                "ColumnName": "LineNumber",
                                "DataType": "int",
                                "AllowNull": true
                            },
                            {
                                "ColumnName": "ColumnNumber",
                                "DataType": "int",
                                "AllowNull": true
                            },
                            {
                                "ColumnName": "Method",
                                "DataType": "nvarchar",
                                "DataLength": 256,
                                "AllowNull": true
                            }
                        ],
                        "disableTriggers": true,
                        "clusteredColumnstoreIndex": false,
                        "primaryKeyColumnName": "Id",
                        "addStandardColumns": [
                            "LogEvent",
                            "TraceId",
                            "SpanId",
                            "SourceContext",
                            "LineNumber",
                            "ColumnNumber",
                            "Method"
                        ],
                        "removeStandardColumns": [
                            "MessageTemplate",
                            "Properties"
                        ],
                        "id": {
                            "nonClusteredIndex": true
                        },
                        "level": {
                            "columnName": "Severity",
                            "storeAsEnum": false
                        },
                        "timeStamp": {
                            "columnName": "Timestamp",
                            "convertToUtc": true
                        },
                        "message": {
                            "columnName": "Msg"
                        },
                        "exception": {
                            "columnName": "Ex"
                        },
                        "messageTemplate": {
                            "columnName": "Template"
                        },
                        "sourceContext": {
                            "columnName": "SourceContext"
                        }
                    }
                }
            },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/etl-hubspot-.txt",
                    "rollingInterval": "Day",
                    "restrictedToMinimumLevel": "Information",
                    "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}"
                }
            }
        ]
    },
    "HubSpot": {
        "BaseUrl": "https://api.hubapi.com",
        "AccessToken": "YOUR_HUBSPOT_ACCESS_TOKEN_HERE"
    },
    "ETL": {
        "BatchSize": 100,
        "MaxRetries": 3,
        "RetryDelaySeconds": 30
    }
}