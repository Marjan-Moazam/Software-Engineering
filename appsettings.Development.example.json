{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=YOUR_SERVER,1433;Database=ETLHubspotDb_Dev;User Id=YOUR_USER;Password=YOUR_PASSWORD;TrustServerCertificate=True;"
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/etl-hubspot-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "HubSpot": {
    "BaseUrl": "https://api.hubapi.com",
    "AccessToken": "YOUR_HUBSPOT_ACCESS_TOKEN_HERE"
  },
  "ETL": {
    "RunOnStartup": true,
    "BatchSize": 100,
    "RebuildDatabase": false
  }
}

